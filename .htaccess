<IfModule mod_rewrite.c>
  RewriteEngine On

  RewriteRule ^clients/add?$ src/View/Clients/add.php [NC,L]
  RewriteRule ^/?$ src/View/Users/login.php [NC,L]
  RewriteRule ^login?$ src/View/Users/login.php [NC,L]
  RewriteRule ^logout?$ src/View/Users/logout.php [NC,L]
  RewriteRule ^users/sign-up?$ src/View/Users/sign-up.php [NC,L]
  RewriteRule ^users/forgot-password?$ src/View/Users/forgot-password.php [NC,L]
</IfModule>